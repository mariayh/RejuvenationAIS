@INPROCEEDINGS{gu2009integrating,
  author = {Gu, F. and Greensmith, J. and Aickelin, U.},
  title = {Integrating real-time analysis with the dendritic cell algorithm
	through segmentation},
  booktitle = {Proceedings of the 11th Annual conference on Genetic and evolutionary
	computation},
  year = {2009},
  pages = {1203--1210},
  organization = {ACM},
  note = {2012-3-17 ~ 3-18},
  file = {两种数据分段（基于抗原的和基于时间的）的方法，增强DCA的实时检测能力:E\:\\000_我的研究\\11- 参考文献\\000_人工免疫系统 AIS\\00_研究组分类论文\\诺丁汉大学       Uwe（英国） _危险理论\\Gu Feng\\(2009) Integrating real-time analysis with the dendritic cell algorithm through segmentation.pdf:PDF},
  review = {提出了两种数据分段（基于抗原的和基于时间的）的方法，增强DCA的实时检测能力.(DC种群的分段)
	
	1、研究动机
	
	 为了实时入侵检测，DCA应该具有实时分析能力；而该文提出时的当下，DCA仍是offline分析。
	
	2、如何找到解决方案的？
	
	 对数据进行分段，定点启用DC群体分析（计算MAC或MCAV、K等）。提出了基于抗原的分段方法（Antigen-based segmentation，ABS）和基于时间的分段方法（Time-based
	segmentation，TBS）。本文采用的分段依据是固定的时间或定量的抗原数。实验结果表明分段思想会使结果有很大的不同。实际上，ABS可以改进入侵识别的结果；分段的实时分析器可以在不降低准确率的情况下，有效地改进DCA的检测效率。
	
	
	3、未来工作
	
	 实现动态分段。原因：本文目前采用的分段方法仍是有一定延时的。应用到现实问题时，在DCA执行检测的过程中，实际情况是在不断动态变化的。因此，为了执行实时分析，需要一种具有online动态性的方法。该方法应该能够在检测过程中自适应调整和演化，以处理未知的新情况。
	
	 该文提出有必要使用实时系统领域中的标准化、规范技术，验证DCA的实时性？比如，使用Duration Calculus[18]来特殊化实时系统的需求，用Timed
	Automata [2]和PLC Automata [5]实现和验证实时系统。
	
	[2] R. Alur and D. L. Dill. A theory of timed automata. Theoretical
	Computer Sicence, 126:183-235, 1994.
	
	[5] H. Dierks. PLC-Automat: A new class of implementable real-time
	automata. Theoretical Computer Sicence, 253:61-93, 2000.
	
	[18] C. Zhou and M. R. Hansen. Duration Calculus: A Formal Approach
	to Real-Time Systems. Springer-Verlag, 2004.}
}

@CONFERENCE{gu2009pca,
  author = {Gu, F. and Greensmith, J. and Oates, R. and Aickelin, U.},
  title = {PCA 4 DCA：The Application of Principal Component Analysis to the
	Dendritic Cell Algorithm},
  year = {2009},
  pages = {266--278},
  book = { Proceedings of the 9th Annual Workshop on Computational Intelligence
	(UKCI 2009), Nottingham, UK },
  file = {PCA for DCA读书笔记:E\:\\000_我的研究\\11- 参考文献\\000_人工免疫系统 AIS\\00_研究组分类论文\\诺丁汉大学       Uwe（英国） _危险理论\\Gu Feng\\(2009) PCA 4 DCA：The Application of Principal Component Analysis to The Dendritic Cell Algorithm读书笔记.docx:Word;PCA主成分分析法:E\:\\000_我的研究\\11- 参考文献\\000_人工免疫系统 AIS\\00_研究组分类论文\\诺丁汉大学       Uwe（英国） _危险理论\\Gu Feng\\(2009) PCA 4 DCA：The Application of Principal Component Analysis to The Dendritic Cell Algorithm.pdf:PDF},
  review = {1、立论
	
	 1）原来信号生成依赖经验和专家知识；要自动生成信号。
	
	 2）现有DCA为了保证实时性，采用了简单的加权求和的信号融合方式，而本意应该用基因调控网络来做，该问题本文未解决。
	
	 抗原：时间戳（本论文属于异常检测范畴，判断数据集中数据项是否异常）；
	
	2、主要内容
	
	 作为基于免疫危险理论的主流算法，DCA在信号定义和危险感知方面的处理方式一向为研究者们所诟病。抛弃了用训练数据定义输入数据的“危险”，DCA基于领域或专家知识来建立输入数据和危险理论中的信号（PAMP、危险和安全信号）间的映射，并使用简单的加权求和方式来进行信号融合，从而判断危险。这种主观和随意的做法不但限制了DCA的应用（尤其当应用于复杂和有噪声的数据集），而且难免会有使数据过度适合DCA的嫌疑。
	
	 因此，本文采用一个基础的特征选择方法（主成分分析法，旨在利用降维的思想，根据数据的变化，把多指标转化为少数几个综合指标，详见MBA智库）来自动地在输入数据和信号间建立映射，并使用生物统计学中的一个数据集（汽车驾驶员压力识别数据集
	）来验证该方法的有效性。
	
	 本文的方法主要分为以下几个步骤（括号内的为本文的具体实现方式及结果）：
	
	1) 归一化：将数据的所有属性归一化到[0,1]范围内（本文使用简单的最小值-最大值归一化方法，主要是因为避免复杂的归一化对PCA的影响）；
	
	2) PCA排序及分类：在选定的属性（除了时间和标识外的其它所有属性）上进行PCA，根据属性值变化程度得出属性排序（EMG、GSR、HR、ECG及Respiration，其中GSR是Foot
	GSR和Hand GSR合并后的综合属性，因为两者是线性相关的）。这个排序将用来抗原生成及信号映射。排序越高，该属性越有可能是恶意抗原数据中的一个属性。（Wilcoxon测试：p<0.05表示无差异）
	
	 抗原频率：EMG（用antigen multiplier生成每项数据的多个副本，频率计算公式为F = 15 + 85 * EMG）
	
	 PAMP信号：ECG和HR
	
	 危险信号：Respiration
	
	 安全信号：GSR
	
	3) 应用DCA：将信号和抗原输入DCA，得到每类抗原的异常系数（信号权重设置如下图所示）。
	
	4）预测评估：将异常系数分为多个区间，分析哪个区间的异常系数是恶意的（K_α在7段中的值）。
	
	5) 真/伪肯定分析：计算各个区间内的真/伪肯定率（不同阈值下的TPR和FPR如下面两个图所示）。
	
	3、今后的工作及展望
	
	本文今后的工作及展望主要分为以下三方面：
	
	 由于不需要人为的干预，本文的方法可以用于实时检测，具体实现是通过阶段性重复本文提出的方法；
	
	 难点：阶段如何确定？如何避免某个参数长期处于异常状态、不变化？（不应该考虑局部变化，而应该考虑有史以来整个趋势）
	
	 本文使用了统计学中的PCA方法实现DCA中的数据自动处理，其它的统计方法，如N-Gram分析，也应该会有较好的效果；
	
	 使用其它类似的数据集对本文的方法进行更全面的验证。
	
	5、启示
	
	本文在立论时提到为了实现DCA的实时性，使用了简单的加权求和的信号融合方式，而本意是使用基因调控网络来实现的，这限制了DCA应用于复杂问题的能力。而作者给出的方法其实不能完全解决这个问题。可不可以从这方面尝试DCA的突破？},
  timestamp = {2012.02.27}
}

@INPROCEEDINGS{roper2009artifical,
  author = {Roper, M.},
  title = {Artifical Immune Systems, Danger Theory, and the Oracle Problem},
  booktitle = {Testing: Academic and Industrial Conference-Practice and Research
	Techniques, 2009. TAIC PART'09.},
  year = {2009},
  pages = {125--126},
  organization = {IEEE},
  abstract = {The oracle problem - the mechanism by which the output associated
	with a test is determined to pass or fail - is an important, but
	frequently neglected, challenge for software testing researchers.
	Artificial immune systems offer a potentially interesting avenue
	of attack but using their power as classifiers to distinguish between
	pass outputs and fail outputs. Danger theory looks for other signals
	to improve this classification. The aim of this paper is to consider
	how AIS and danger theory may be applied to the oracle problem.}
}

